{
    "webserver_port": 8080,
    "logfile": "ENV:PROXY_ALL_LOG:/log/all.log",
    "auth": {
	"username": "ENV:PROXY_WEBSERVER_USER:admin",
	"password": "ENV:PROXY_WEBSERVER_PASS:password"
    },
    "logger": {
	"level": "debug",
	"filename": "/tmp/proxy.log"
    },
    "smtp": {
	"from": "ENV:PROXY_SMTP_FROM:events@newco.com",
	"auth": {
	    "user": "ENV:PROXY_SMTP_USER:admin",
	    "pass": "ENV:PROXY_SMTP_PASS:password"
	},
	"options": {
	    "port": "ENV:PROXY_SMTP_PORT:465",
	    "host": "ENV:PROXY_SMTP_HOST:smpt.gmail.com",
	    "secure": true,
	    "ignoreTLS": false,
	    "requireTLS": true,
	    "name": "logstash-proxy",
	    "localAddress": "0.0.0.0",
	    "connectionTimeout": 3000,
	    "greetingTimeout": 3000,
	    "socketTimeout": 5000,
	    "debug": true,
	    "lmtp": false,
	    "authMethod": "ENV:PROXY_SMTP_AUTH:PLAIN",
	    "tls": {}
	}
    },
    "proxy": [
	{ "from": 2020, "to": { "port": 5000, "host": "logs" }, "protocol": "tcp", "ignore": false },
	{ "from": 2020, "to": { "port": 5000, "host": "logs" }, "protocol": "udp", "ignore": false },
	{ "from": 2021, "to": { "port": 5001, "host": "logs" }, "protocol": "tcp", "ignore": false },
	{ "from": 2021, "to": { "port": 5001, "host": "logs" }, "protocol": "udp", "ignore": false }
    ],
    "db": {
	"connection": {
	    "filename": "ENV:PROXY_SQLITEDB:/tmp/logger.db"
	}
    }
}
